<tr>
  <td><%= test.title %></td>
  <td><%= test.category.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.size %></td>
  <td><%= link_to t('admin.actions.show'), admin_test_path(test) %></td>
  <td><%= link_to t('admin.actions.edit'), edit_admin_test_path(test) %></td>
  <td><%= link_to t('admin.actions.delete'), 
        admin_test_path(test), 
        method: :delete,
        data: { confirm: t('admin.actions.confirm') }
      %>
  </td>
</tr>