<tr>
  <td><%= test.title %></td>
  <td><%= test.category.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.size %></td>
  <td><%= link_to "Посмотреть", admin_test_path(test) %></td>
  <td><%= link_to "Редактировать", edit_admin_test_path(test) %></td>
  <td><%= link_to "Удалить", 
        admin_test_path(test), 
        method: :delete,
        data: { confirm: 'Вы уверены?' }
      %>
  </td>
</tr>